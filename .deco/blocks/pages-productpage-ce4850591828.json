                }
              },
              "title": "COMPLETE SEU LOOK",
              "layout": {
                "showArrows": true,
                "showDots": true,
                "headerAlignment": "left"
              }
            }
          },
          {
            "__resolveType": "website/sections/Rendering/Lazy.tsx",
            "section": {
              "__resolveType": "Benefits - 01"
            }
          },
          {
            "__resolveType": "website/sections/Rendering/Lazy.tsx",
            "section": {
              "__resolveType": "Footer - 02"
            }
          }
        ]
