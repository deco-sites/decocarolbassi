          },
          {
            "title": "Facebook",
            "icon": "https://ozksgdmyrqcxcwhnbepg.supabase.co/storage/v1/object/public/assets/9166/743cc7f0-edd6-4e03-8408-7b6a2db92636",
            "href": "https://www.facebook.com/sharer/sharer.php?u={pageUrl}"
